{
  "name": "@abb/controller",
  "version": "1.0.0",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "license": "ISC",
  "scripts": {
    "build": "rimraf ./dist && tsc",
    "gen:types": "apollo client:codegen --includes=./src/**/*.tsx --endpoint=http://localhost:4000 --tagName=gql --target=typescript --outputFlat=./src/schemaTypes.ts",
    "refresh:types": "npm run gen:types && npm run build"
  },
  "dependencies": {
    "graphql": "^14.3.1",
    "graphql-tag": "^2.10.3",
    "react": "^16.13.1",
    "react-apollo": "^3.1.5",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "apollo": "^2.28.0",
    "rimraf": "^3.0.2",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^5.0.0",
    "typescript": "^3.7.5"
  }
}
